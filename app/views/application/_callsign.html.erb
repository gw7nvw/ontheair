<span title="<%=DxccPrefix.name_from_call(this_callsign.upcase.strip)%>"><%user=User.find_by_full_callsign(this_callsign.strip)%>
   <% if user then %>
     <%=link_to this_callsign.upcase, "users/"+user.callsign, remote: true,  :id => 'link', :onclick => "linkHandler('link')" %>
   <% elsif ["ZLOTA","PNP","SMS"].include?(this_callsign.upcase.strip) then %>
     <%=this_callsign.upcase.strip%>
   <% else%>
     <%=link_to this_callsign.upcase.strip, 'https://qrz.com/db/'+this_callsign.upcase.strip, target: "_blank", class: "non_member" %>&nbsp<img src="/assets/external-link-symbol-12.png">
   <% end %>
</span>
